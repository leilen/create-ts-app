{
  "name": "create-ts-cli",
  "version": "1.0.0",
  "description": "Create basic type script cli app with webpack.",
  "keywords": [
    "typescript",
    "create"
  ],
  "homepage": "https://github.com/leilen/create-ts-app",
  "main": "dist/app.js",
  "scripts": {
    "build": "webpack -p --mode=production",
    "pro": "node dist/app.js",
    "start": "ts-node src/app.ts",
    "clean": "rm -rf ./dist/* && touch ./dist/.keep"
  },
  "author": "Jin Hyeok Yu",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "create-ts-app": "dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leilen/create-ts-app.git"
  },
  "devDependencies": {
    "@types/node": "^12.7.8",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "ts-loader": "^6.2.0",
    "webpack": "^4.41.0",
    "webpack-node-externals": "^1.7.2"
  }
}
